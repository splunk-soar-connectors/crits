                "query": {
                    "description": "Query to run",
                    "data_type": "string",
                    "order": 1,
                    "primary": true,
                    "contains": [
                        "crits query"
                    ]
                },
                "limit": {
                    "description": "Results per page",
                    "data_type": "numeric",
                    "order": 2
                },
                "offset": {
                    "description": "Paging offset",
